{
    "version": 2,
    "builds": [
      {
        "src": "index.js", 
        "use": "@vercel/node"
      },{
        "src": "frontend/dist/**",
        "use": "@vercel/static",
        "config": { "includeFiles": ["frontend/dist/**"] }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/",
        "dest": "frontend/dist/index.html"
      },
      {
        "src": "/(.+)",
        "dest": "frontend/dist/$1"
      }
    ]
  }